<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="root" maxHeight="1.7976931348623157E308" maxWidth="350.0" minWidth="350.0" styleClass="notification"
          xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES"/>
        <ColumnConstraints hgrow="NEVER"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="1.7976931348623157E308" vgrow="ALWAYS"/>
        <RowConstraints minHeight="0.0" vgrow="NEVER"/>
    </rowConstraints>
    <children>
        <HBox GridPane.columnSpan="2147483647" GridPane.rowIndex="1">
            <children>
                <CheckBox fx:id="dontShowAgainBox" mnemonicParsing="false" text="Diese Meldung nicht mehr anzeigen."
                          GridPane.columnSpan="2147483647" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                    <font>
                        <Font size="11.0"/>
                    </font>
                    <HBox.margin>
                        <Insets left="15.0"/>
                    </HBox.margin>
                </CheckBox>
            </children>
            <GridPane.margin>
                <Insets bottom="5.0" left="10.0" right="10.0"/>
            </GridPane.margin>
            <padding>
                <Insets top="5.0"/>
            </padding>
        </HBox>
        <HBox fx:id="contentBox" GridPane.vgrow="ALWAYS">
            <GridPane.margin>
                <Insets left="10.0" top="10.0"/>
            </GridPane.margin>
        </HBox>
        <Pane id="exitPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="20.0"
              styleClass="exitPane" GridPane.columnIndex="1" GridPane.rowSpan="2147483647">
            <children>
                <Button fx:id="exitButton" alignment="CENTER" maxHeight="20.0" maxWidth="20.0" minHeight="20.0"
                        minWidth="20.0" mnemonicParsing="false" onAction="#closeNotification" styleClass="exitButton"
                        text="X" textAlignment="CENTER">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </Button>
            </children>
        </Pane>
    </children>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
</GridPane>
